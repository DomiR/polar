    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "default": "./dist/providers/index.js"
    }
  },
  "devDependencies": {
    "terser": "^5.43.1",
    "tsconfig": "workspace:*",
    "tsup": "^8.5.0",
    "typescript": "latest"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "gpt-tokenizer": "^3.0.1"
