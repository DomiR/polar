{
  "name": "@polar-sh/models",
  "description": "Polar LLM Models",
  "author": "Polar",
  "license": "Apache-2.0",
  "type": "module",
  "version": "0.1.12",
  "repository": {
    "type": "git",
    "url": "https://github.com/polarsource/polar.git",
    "directory": "clients/packages/checkout"
  },
  "scripts": {
    "build": "rm -rf dist/ && tsup"
  },
  "exports": {
